#include <iostream>



using namespace std;

int main()


{

    short int num1, num2;

    cout<<"ingresa un primer numero: ";

    cin>>num1;

    cout<<"ingresa un segundo numero: ";

    cin>>num2;

    

    if(num1%num2==0)

        cout<<num2 <<" es multiplo de " <<num1;

    else

    {

        if(num2%num1==0)

            cout<<num1 <<" es multiplo de " <<num2;

        else

            cout<<"los numeros no son multiplos";

        

    }

    return 0;

    

}

